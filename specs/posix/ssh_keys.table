
table_name("ssh_keys")

description("Returns the id_rsa file for users and whether or not they are encrypted")

schema([
    Column("uid", BIGINT, "The local user that owns the ssh_keys file",
      additional=True),
    Column("key_file", TEXT, "Path to key file"),
    Column("encrypted", INTEGER, "1 if key is encrypted, 0 otherwise"),
    ForeignKey(column="uid", table="users"),
])

implementation("ssh_keys@getSshKeys")

